{
    "name": "thies/json-web-token",
    "type": "library",
    "description": "JSON Web Token library written in PHP",
    "keywords": ["JWT", "Token"],
    "homepage": "https://github.com/thies4321/json-web-token",
    "license": "MIT",
    "authors": [
        {
            "name": "Thies Verhave",
            "homepage": "https://verhave.net/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "JsonWebToken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonWebToken\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/json-web-token"],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3",
        "psy/psysh": "@stable"
    }
}
