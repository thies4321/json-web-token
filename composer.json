{
    "name": "thies/json-web-token",
    "type": "standard",
    "description": "JSON Web Token library written in PHP",
    "homepage": "https://github.com/thies4321/json-web-token",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JsonWebToken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonWebToken\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/json-web-token"],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "symfony/console": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
